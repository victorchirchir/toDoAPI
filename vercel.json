{
    "version": 2,
    "builds": [
      {
        "src": "api/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb",  "runtime": "git+https://github.com/jperelli/vercel-python-gis.git#main@1.0.0" }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles_build"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "api/wsgi.py"
      }
    ]
  }

  